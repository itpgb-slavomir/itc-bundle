<?xml version="1.0" ?>
<container
	xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>

		<service
			id="sk.itcbundle.code.generator.rest.command"
			class="%sk.itcbundle.code.generator.rest.command.class%">

			<argument
				type="service"
				id="logger" />

			<argument
				type="service"
				id="sk.itcbundle.code.reflection" />

		</service>
		
		<service
			id="sk.itcbundle.code.generator.rest.controller"
			class="%sk.itcbundle.code.generator.rest.controller.class%">

			<argument
				type="service"
				id="logger" />

			<argument
				type="service"
				id="sk.itcbundle.code.reflection" />

		</service>
		
		<service
			id="sk.itcbundle.code.generator.rest.entity"
			class="%sk.itcbundle.code.generator.rest.entity.class%">

			<argument
				type="service"
				id="logger" />

			<argument
				type="service"
				id="sk.itcbundle.code.reflection" />

		</service>
		
		<service
			id="sk.itcbundle.code.generator.rest.form"
			class="%sk.itcbundle.code.generator.rest.form.class%">

			<argument
				type="service"
				id="logger" />

			<argument
				type="service"
				id="sk.itcbundle.code.reflection" />

		</service>

	</services>

</container>