{
	"name" : "sk.itcloud.symfony/itc-bundle",
	"license" : "ITCloud Symfony Bundle ITC",
	"type" : "bundle",
	"description" : "Symfony ITC Bundle",
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-0" : {
			"SK\\ITCBundle" : "src/"
		}
	},
	"bin" : [
		"bin/itc",
		"bin/itc-socket"
	],
	"config" : {
		"bin-dir" : "bin",
		"vendor-dir" : "vendor"
	},
	"require" : {
		"php" : ">=5.5.0",
		"doctrine/orm" : "2.5.2",
		"doctrine/doctrine-bundle" : "1.6.1",
		"doctrine/doctrine-cache-bundle" : "1.2.2",
		"doctrine/doctrine-migrations-bundle" : "1.1.1",
		"doctrine/cache" : "1.5.4",
		"doctrine/annotations" : "1.2.7",
		"doctrine/dbal" : "2.5.2",
		"doctrine/migrations" : "1.2.1",
		"symfony/form" : "3.0.0",
		"symfony/routing" : "3.0.0",
		"symfony/http-foundation" : "3.0.0",
		"symfony/assetic-bundle" : "2.7.1",
		"symfony/security" : "^3.0",
		"symfony/security-http" : "3.0.0",
		"symfony/security-csrf" : "3.0.0",
		"symfony/security-bundle" : "3.0.0",
		"symfony/framework-bundle" : "3.0.0",
		"symfony/monolog-bundle" : "^2.8",
		"symfony/swiftmailer-bundle" : "^2.3",
		"symfony/twig-bundle" : "3.0.1",
		"packagist/yuicompressor-bin" : "2.4.8",
		"twig/twig" : "1.23.3"
	},
	"require-dev" : {
		"symfony/console" : "3.0.0",
		"symfony/finder" : "^3.0",
		"symfony/process" : "^3.0",
		"symfony/event-dispatcher" : "^3.0",
		"symfony/debug" : "3.0.0",
		"zendframework/zend-code" : "2.6.1",
		"zendframework/zend-soap" : "2.5.1",
		"behat/behat" : "dev-master",
		"andrewsville/php-token-reflection" : "^1.4",
		"sonata-project/formatter-bundle" : "^2.3",
		"cboden/ratchet" : "^0.3.4",
		"react/zmq" : "0.2.*|0.3.*",
		"jenner/react-multi-process" : "^0.1.0",
		"zerkalica/phpuml" : "dev-master",
		"sauce/sausage" : "*",
		"jms/serializer-bundle" : "^1.1"
	}
}